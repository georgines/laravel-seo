{
    "name": "werxe/laravel-seo",
    "description": "Seo management for Eloquent models.",
    "keywords": [ "php", "laravel", "seo" ],
    "homepage": "http://werxe.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Bruno Gaspar",
            "email": "bruno@werxe.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "~5.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "classmap": [ "src/migrations" ],
        "psr-4": { "Werxe\\LaravelSeo\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Werxe\\LaravelSeo\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
